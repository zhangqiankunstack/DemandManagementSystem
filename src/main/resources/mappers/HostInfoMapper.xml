<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zb.mapper.HostInfoMapper">

		<!-- 通用查询映射结果 -->
		<resultMap id="BaseResultMap" type="com.rengu.entity.HostInfoModel">
					<id column="id" property="id"/>
					<result column="host_ip" property="hostIp"/>
					<result column="port" property="port"/>
					<result column="username" property="username"/>
					<result column="password" property="password"/>
					<result column="new_database" property="newDatabase"/>
					<result column="status" property="status"/>
					<result column="name" property="name"/>
		</resultMap>

		<!-- 通用查询结果列 -->
		<sql id="Base_Column_List">
            id, host_ip, port, username, password, new_database, status, name
		</sql>

</mapper>
